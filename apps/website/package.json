{
  "name": "@alveusgg/alveusgg-website",
  "version": "0.1.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alveusgg/alveusgg.git",
    "directory": "apps/website"
  },
  "bugs": {
    "url": "https://github.com/alveusgg/alveusgg/issues"
  },
  "engines": {
    "node": "24.x"
  },
  "types": "dist/trpc.d.ts",
  "scripts": {
    "build:next": "next build --webpack",
    "build:next-sitemap": "next-sitemap --config next-sitemap.config.js",
    "build:push-sw": "tsup --config sw.tsup.config.ts",
    "build:trpc-types": "tsup --config trpc.tsup.config.ts",
    "build:manifest": "tsx ./build-scripts/create-manifest.ts",
    "build:presets": "tsx ./build-scripts/update-presets.ts",
    "dev:next": "next dev --webpack",
    "dev:push-sw": "tsup --watch --config sw.tsup.config.ts",
    "dev:trpc-types": "tsup --watch --config trpc.tsup.config.ts",
    "build": "pnpm run --sequential \"/^build:(?!presets$).*$/\"",
    "dev": "pnpm run \"/^(build:manifest|dev:.*)/\"",
    "postinstall": "pnpm build:trpc-types",
    "lint": "next typegen && eslint --max-warnings 0",
    "types": "next typegen && tsc --noEmit",
    "start": "next start",
    "test": "vitest"
  },
  "dependencies": {
    "@alveusgg/data": "0.71.0",
    "@alveusgg/database": "workspace:*",
    "@alveusgg/donations-core": "workspace:*",
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@bart-krakowski/get-week-info-polyfill": "^1.0.8",
    "@cloudflare/stream-react": "^1.9.2",
    "@dagrejs/dagre": "^1.1.8",
    "@eyevinn/webrtc-player": "^0.13.0",
    "@floating-ui/react": "^0.27.17",
    "@fontsource/pt-sans": "^5.2.8",
    "@headlessui/react": "^2.2.9",
    "@iframe-resizer/react": "^5.5.9",
    "@maplibre/maplibre-gl-geocoder": "^1.9.4",
    "@next/env": "^16.1.6",
    "@paralleldrive/cuid2": "^3.3.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@trpc/client": "^11.9.0",
    "@trpc/next": "^11.9.0",
    "@trpc/react-query": "^11.9.0",
    "@trpc/server": "^11.9.0",
    "@twurple/chat": "^8.0.3",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "@vercel/analytics": "^1.6.1",
    "@vercel/functions": "^3.4.1",
    "@xyflow/react": "^12.10.0",
    "country-list": "^2.4.1",
    "csv-stringify": "^6.6.0",
    "dompurify": "^3.3.1",
    "fast-xml-parser": "^5.3.4",
    "feed": "^5.2.0",
    "heic-to": "^1.4.2",
    "html-react-parser": "^5.2.17",
    "jsdom": "^27.4.0",
    "linkify-it": "^5.0.0",
    "lodash": "^4.17.23",
    "luxon": "^3.7.2",
    "maplibre-gl": "^5.17.0",
    "motion": "^12.33.0",
    "next": "^16.1.6",
    "next-auth": "^4.24.13",
    "next-sitemap": "^4.2.3",
    "next-superjson": "^1.0.7",
    "oauth": "^0.10.2",
    "partysocket": "^1.1.11",
    "pluralize": "^8.0.0",
    "probe-image-size": "^7.2.3",
    "raw-body": "^3.0.2",
    "react": "^19.2.4",
    "react-aria": "^3.46.0",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-markdown": "^10.1.0",
    "react-quill-new": "^3.8.3",
    "react-stately": "^3.44.0",
    "react-textarea-autosize": "^8.5.9",
    "react-to-text": "^2.0.1",
    "remark-heading-id": "^1.0.1",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "smooth-scroll-into-view-if-needed": "^2.0.2",
    "superjson": "^2.2.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.12.2",
    "@eslint/js": "^9.39.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@next/eslint-plugin-next": "^16.1.6",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/country-list": "^2.1.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/jsdom": "^27.0.0",
    "@types/linkify-it": "^5.0.0",
    "@types/lodash": "^4.17.23",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.12",
    "@types/oauth": "^0.9.6",
    "@types/pluralize": "^0.0.33",
    "@types/probe-image-size": "^7.2.5",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/serviceworker": "^0.0.183",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "file-type": "^21.3.0",
    "globals": "^17.3.0",
    "loader-utils": "^3.3.1",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "ts-morph": "^27.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18",
    "webpack": "^5.105.0"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "./src/styles/tailwind.css",
    "tailwindFunctions": [
      "classes"
    ],
    "importOrder": [
      "<BUILTIN_MODULES>",
      "<THIRD_PARTY_MODULES>",
      "^@alveusgg/data(/.+)?$",
      "^@alveusgg/database(/.+)?$",
      "^@alveusgg/donations-core(/.+)?$",
      "^@/env(/.+)?$",
      "^@/server(/.+)?$",
      "^@/data(/.+)?$",
      "^@/utils(/.+)?$",
      "^@/hooks(/.+)?$",
      "^@/components(/.+)?$",
      "^@/icons(/.+)?$",
      "^@/assets(/.+)?$",
      "^@/.+$",
      "^\\."
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderSideEffects": false
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {
        "optimize": true
      }
    }
  }
}
